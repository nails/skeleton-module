{
    "name": "{vendor}/{name}",
    "keywords": [],
    "homepage": "{url}",
    "description": "{description}",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "nails/common": "dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit": "8.*"
    },
    "autoload": {
